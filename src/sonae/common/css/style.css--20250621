  -webkit-clip-path: polygon(0 0, 0 2px, 10px 2px, 10px 12px, 12px 12px, 12px 0);
          clip-path: polygon(0 0, 0 2px, 10px 2px, 10px 12px, 12px 12px, 12px 0);
